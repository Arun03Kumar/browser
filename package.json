{
  "name": "browser",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "electron/main.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:vite\" \"npm run dev:electron:watch\"",
    "dev:vite": "vite",
    "dev:electron": "npx wait-on http://localhost:5173 && electron .",
    "dev:electron:watch": "npx wait-on http://localhost:5173 && nodemon --exec \"electron .\" --watch \"electron/\"",
    "build": "vite build",
    "start": "electron ."
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.11",
    "concurrently": "^9.2.0",
    "lucide-react": "^0.539.0",
    "nodemon": "^3.1.10",
    "quickjs-emscripten": "^0.31.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "tailwindcss": "^4.1.11"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.0",
    "electron": "^37.2.6",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "vite": "^7.1.2",
    "wait-on": "^8.0.4"
  }
}
